<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    <class name="alan.share.skillacademy.model.SkillAcademyCategory" table="SkillAcademyCategory">
    <id name="skillcategoryid">
    <generator class="native"/>  <!-- 主键生成策略，自增-->
    </id>
    <property name="skillcategoryname"/>
    <property name="skillcategorydescription"/>
    <property name="skillcategoryimage"/>
    </class>
    </hibernate-mapping>
