<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    <class name="alan.share.user.model.BestChoose" table="BestChoose">
    <id name="bestid">
    <generator class="assigned"/>  <!-- 主键生成策略，app赋值-->
    </id>
    <property name="bestname"/>
    <property name="bestcontent"/>
    <property name="bestplaintext"/>
    <property name="bestdate"/>
    <property name="bestishot"/>
    <property name="bestcover"/>
    <property name="bestclickcount"/>
    </class>
    </hibernate-mapping>
