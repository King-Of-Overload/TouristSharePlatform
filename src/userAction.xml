<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<package name="tripuser" extends="welcome">
<global-results>
<result name="msg">/WEB-INF/page/reg.jsp</result>
<result name="loginPage">/WEB-INF/page/login.jsp</result>
<result name="quit" type="redirectAction">index</result>
<result name="pageNotFound">/WEB-INF/page/404.jsp</result>
</global-results>

<action name="tripuser_*" class="tripUserAction" method="{1}">
<result name="registerPage">/WEB-INF/page/reg.jsp</result>
<result name="space">/WEB-INF/page/space.jsp</result>
<result name="user">/WEB-INF/page/user.jsp</result>
<result name="goToUserSpace">/WEB-INF/page/userspace.jsp</result>
<result name="goToPersonalDoc">/WEB-INF/page/PersonalDocument.jsp</result>
<result name="goToList">/list.html</result>
</action>
</package>
</struts>
